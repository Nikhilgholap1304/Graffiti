{
  "name": "@bedrock-layout/use-forwarded-ref",
  "version": "1.6.1",
  "description": "bedrock-layout useForwardedRef",
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "useForwardedRef",
    "react",
    "hook",
    "useRef"
  ],
  "author": "Jarvis1010 <travis.mair@gmail.com>",
